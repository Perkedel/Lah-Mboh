ACTOR D2C_Pipebomb : Weapon 25018
{
   Weapon.SlotNumber 0
   Weapon.SelectionOrder 25018
   Inventory.PickupSound "misc/w_pkup"
   Inventory.PickupMessage "You got a pipebomb!"
   Weapon.AmmoType "D2C_PipebombAmmo"
   Weapon.AmmoGive 1
   Weapon.AmmoUse 1
   Weapon.Kickback 0
   AttackSound "weapons/pbarm"
   Scale 0.1
   + NOALERT
   States
   {
   Spawn:
      PIPB A -1
      Loop
   Ready:
      PIPE A 1 A_WeaponReady
      Loop
   Deselect:
      PIPE A 1 A_Lower
      Loop
   Select:
      PIPE A 1 A_Raise
      Loop
   Fire:
      PIPE BCDEFGH 1
      PIPE Z 8
      PIPE IJKLMNOP 1
      PIPE P 0 A_FireCustomMissile("Pipebomb",0,1,0,0)
      PIPE Q 8 A_PlayWeaponSound("weapons/pbthrow")
      PIPE RSTU 1
      PIPE Z 8
      PIPE P 0 A_GiveInventory("D2C_Detonator")
      PIPE P 0 A_TakeInventory("D2C_Pipebomb")
      PIPE HGFEDCB 1
      PIPE A 4
      PIPE A 4 A_ReFire
      Goto Ready
   }
}

